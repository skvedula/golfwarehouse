#WC_AccountDisplay_div_1.sign_in_registration {
  .header_return, .header_new {
    background-color: #fff;
    padding: 0px;
    margin: 10px 0;
  }

  .checkbox_label_registration {
    label {
      padding-left: 5px;
    }
  }

  .forgot_password_link_padding {
    margin-top: 3px;
  }

  .button_text {
    font-weight: normal;
    font-size: 1.4rem;
    letter-spacing: 1px;
  }

  .header_new {
    border-top: 1px solid $secondary-color;
    padding-top: 20px;

    .button_align {
      margin-top: 20px;
      text-align: right !important;
    }

    .button_primary {
      width: 130px !important;
      font-weight: normal;
      letter-spacing: 1px;
    }

    p {
      margin-bottom: 10px;
    }

    li {
      margin-bottom: 5px;
    }
  }
}

.my_account {
  margin-top: 5px;

  .main_header {
    border: none !important;
    background: none !important;
    padding: 0px !important;
    text-align: center;
    font-size: 1.8rem !important;
    color: #000 !important;
    margin-bottom: 20px;
  }

  .body {
    padding: 0px !important;
  }

  .myaccount_desc_title {
    display: flex;
    font-size: 1.6rem;
    padding: 0 10px 10px 10px;
    color: #000;
    font-weight: normal;
    border-bottom: 1px solid $secondary-color;


    .sign-out {
      font-size: 1.4rem;
      color: $primary-color;
      text-align: right;
      flex: 2;
      align-self: center;
    }

    .welcome {
      flex: 8;
      align-self: center;
    }
  }

  .myaccount_section_header {
    border: none;
    background: none;
    padding: 0 10px;

    .header {
      padding-left: 0px;
      color: #000;
      font-size: 1.6rem;
    }
  }

  .content, .info_table {
    border: none;
    display: block;
  }

  .info {
    padding: 0px 10px;
    margin-bottom: 10px;

    > p {
      text-align: right;
      font-size: 1.4rem;
    }
  }

  .table-row {
    display: flex;
    margin-bottom: 3px;
  }

  .label {
    border: none;
    background-color: #fff !important;
    flex: 2;
    text-align: right;
    color: $product-sortby-color;
    max-width: 60px;
  }

  .info_content {
    flex: 8;
    padding-left 10px;
  }

}

.my-account-nav {
  margin-bottom: -5px;
  fieldset {
    border: none;
    padding: 0px;
    margin: 0px

    a {
      font-size: 1.4rem;
    }

    li {
      border-bottom: none;
    }
  }

  div[role="button"] {
    background-color: #fff;
    height: 48px;
    line-height: 48px;
    margin: 0px;
    border-bottom: none;
    border-top: 1px solid $secondary-color;
  }

  .arrow {
    margin: 17px 5px 0 0;
  }
}

