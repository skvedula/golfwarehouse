}

#addressId_menu {
  font-size: 1.3rem !important;
}

#addr_title {
  padding-left: 10px;

  h2 {
    font-size: 1.3rem;
  }
}

#addressIdRefreshArea {
  fieldset {
    border: none;
    margin: 0px;
  }

  .inputField {
    width: 100%;
    padding: 0px 8px;
  }

  .column_label {
    margin-top: 10px;
  }

  select {
    margin-bottom: 10px;
  }
}

.my-account-breadcrumbs {
  margin-top: 10px !important;
}

.return-msg {
  padding: 5px;
  font-weight: bold;
  border-top: $acct-border;
}

.guestOrderDetail {
  .body {
    .content {
      padding: 0;
    }
  }
}

.order-detail-item {
  margin-bottom: 10px;
  padding: 5px;
  border-bottom: $acct-border;
  &:last-of-type {
    margin-bottom: 0;
  }
  &:first-of-type {
    border-top: $acct-border;
  }
  .img-title {
    display: flex;
    margin-bottom: 10px;
    .item-image {
      display: block;
      width: 30%;
    }
    .item-specs {
      display: block;
      width: 70%;
      margin-left: 10px;
      .strong_content {
        font-weight: bold;
      }
    }
  }
  .detail-info {
    ul {
      padding-left: 0;
      list-style-type: none;
      li {
        .tracking-info {
          a {
            color: #1ba8de;
            &:last-of-type {
              display: inline-block;
              margin: 10px 0;
            }
          }
        }
        strong {
          text-transform: capitalize;
          &.date:after {
            content: 'Return Request Date: ';
          }
          &.QTY:after {
            content: 'Quantity: ';
          }
          &.status:after {
            content: 'Status: ';
          }
          &.tracking:after {
            content: 'Tracking: ';
          }
          &.tracking-number:after {
            content: 'Tracking Number: ';
          }
          &.each:after {
            content: 'Each: ';
          }
          &.total:after {
            content: 'Total: ';
          }
        }
        p {
          display: inline-block;
        }
      }
    }
  }
}

.order-detail-sub-total {
  text-align: right;
  font-size: 1.5rem;
  padding-right: 10px;
  .strong {
    color: #000;
  }
}
